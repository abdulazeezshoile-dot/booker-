{
  "expo": {
    "name": "Booker",
    "slug": "booker-inventory",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a73e8"
    },
    "platforms": ["ios", "android"],
    "scheme": "bookerapp",
    "sdkVersion": "49.0.0",
    "runtimeVersion": "1.0.0",
    "updates": {
      "fallbackToCacheOnError": true,
      "url": "https://u.expo.dev/your-project-id"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.bookerapp.inventory",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "useNextNotificationFormat": true,
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.bookerapp.inventory",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera is used to capture inventory photos",
        "UIRequiredDeviceCapabilities": ["armv7"]
      },
      "associatedDomains": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Inventory management app for managing stock, tracking transactions, and generating reports",
    "privacy": "https://example.com/privacy",
    "primaryColor": "#1a73e8",
    "backgroundColor": "#ffffff",
    "owner": "your-username",
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
